<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ConsultaPonderacion">

    <include
        android:id="@+id/tooPonderacion"
        layout="@layout/barra_titulo"/>

    <FrameLayout
        android:id="@+id/frame_ponderacion"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tooPonderacion"
        android:layout_above="@id/nav_ponderacion"/>

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/nav_ponderacion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:itemBackground="@android:color/white"
        app:itemIconTint="@drawable/pressed"
        app:itemTextColor="@drawable/pressed"
        app:menu="@menu/nav_ponderacion" />

</RelativeLayout>